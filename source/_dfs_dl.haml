-# local 'node'
- if node.kind_of?(Array)
  - node.each do |v|
    %dd
      =partial("dfs_dl", locals: { node: v })
- elsif node.kind_of?(Hash)
  - if node.has_key?("key")
    =partial("parameter", locals: { parameter: node })
  - else
    %dl
      - node.each do |k, v|
        %dt
          =k.to_s
        %dd
          =partial("dfs_dl", locals: { node: v })
- else
  =h node.to_s
